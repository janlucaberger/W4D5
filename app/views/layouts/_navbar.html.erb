<nav>
  <h1><a href="<%= bands_url %>">Music App</a></h1>
  <div class="log-sign-container">
    <% if current_user %>
      <span>Welcome </span>
      <span>&nbsp <%= current_user ? current_user.email : "" %></span> 

      <form class="" action="<%= session_url %>" method="post">
        <%= auth_token %>
        <input type="hidden" name="_method" value="DELETE">
        <input type="submit" name="" value="LOGOUT">
      </form>
    <% else %>
      <form class="" action="<%= session_url %>" method="post">
        <%= auth_token %>
        <input placeholder="Email" type="text" name="user[email]" value="">
        <input placeholder="Password" type="text" name="user[password]" value="">
        <input type="submit" name="" value="LOGIN">
      </form>
      <a class="button" href="<%= new_user_url %> ">SIGN UP</a>
    <% end %>
  </div>
</nav>
